	<script type="text/javascript">
		var zjrkUuid = 1903;
		var rkday = 21;
		$.ajax({
			url:"/user/loginStatus",
			dataType:"json",
			success:function(ret){
				if(ret.code == "10001"){
					if(ret.model.id == 2){
						$("#login").text("ddddddkdk:"+returnCitySN["cname"]+"xxx"+ret.model.jgName);
						$(".youke-power").show();
						$("#other_menu_yk").show();
					}else{
						$("#login").text("mimdikmdsksks:"+ret.model.jgName);
						if(ret.model.id == 1){
							$("#jangle_menu_xtgl").show();
							$("#other_menu_yygl").show();
						}else{
							$("#other_menu_yygl").show();
						}
					}
					$("#login").append("<a href='javascript:logout();'><img alt='ddd' src='images/userLogout.png' width='42' height='42'></a>");
					autoAudio();
				}
			},
			error : function(request, textStatus, errorThrown) {
				jangleShowAjaxError(request, textStatus, errorThrown);
			}
		});
		$.ajax({
			url:"/visitCtrl/insert",
			type:"POST",
			dataType:"json",
			data:{"dmDesc":returnCitySN["cip"],"dmDesc2":returnCitySN["cname"]},
			success:function(ret){
				console.log(ret);
				$("#visitCount").text("mms"+ret.count+"ssss");
			},error:function(){}
		});

		function logout(){
			$.ajax({
				url:"/user/logout",
				dataType:"json",
				success:function(ret){
					if(ret.code =="10001"){
						alert(ret.message);
						location.href="index.html";
					}
				},
				error : function(request, textStatus, errorThrown) {
					jangleShowAjaxError(request, textStatus, errorThrown);
				}
			});
		}

		function zjrkBack(){
			zjrkUuid -= 5;
			rkday--;
			$("#zjrkURL").text("2019-10-"+rkday);
			$("#zjrkURL").attr("href","http://www.zjrjks.org/weChat.do?method=pInfo&uuid="+zjrkUuid);
		}

		function zjrkNext(){
			zjrkUuid += 5;
			rkday++;
			$("#zjrkURL").text("2019-10-"+rkday);
			$("#zjrkURL").attr("href","http://www.zjrjks.org/weChat.do?method=pInfo&uuid="+zjrkUuid);
		}
	</script>
	<script type="text/javascript">
		var musicArr = {};
		var musicNum = 1;
		function autoAudio(){
			$.ajax({
				url:"/bsAttachmentCtrl/selectPage.ctrl?attSourceId=427&attSourceType=bs_test&status=1",
				dataType:"json",
				success:function(ret){
					if(ret != null && ret.code =="10001"){
						musicArr = ret.list;
						var audio = new Audio();
						audio.preload = true;
						audio.src = "/bsAttachmentCtrl/downLoadByYan.ctrl?id="+musicArr[0].id;
						audio.addEventListener('ended', playEndedHandler,false);
						audio.play();
						function playEndedHandler(){
							var i = musicNum++ % musicArr.length;
							audio.src = "/bsAttachmentCtrl/downLoadByYan.ctrl?id="+musicArr[i].id;
							audio.play();
						}
					}
				},error:function(){
				}
			});
		}
	</script>
